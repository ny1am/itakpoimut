{
  "name": "itakpoimut",
  "version": "0.3.5",
  "description": "itakpoimut: volunteer project",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "heroku-postbuild": "bower install && gulp prod"
  },
  "cacheDirectories": [
    "node_modules",
    "public/js/libs"
  ],
  "browser": {
    "modernizr": "./public/js/thirdparty/modernizr-custom.js",
    "jquery": "./public/js/libs/jquery/dist/jquery.min.js",
    "jquery.chosen": "./public/js/thirdparty/chosen.jquery.js",
    "jquery.throttle-debounce": "./public/js/libs/jquery-throttle-debounce/jquery.ba-throttle-debounce.min.js"
  },
  "browserify-shim": {
    "modernizr": {
      "exports": "Modernizr"
    },
    "jquery": {
      "exports": "$"
    },
    "jquery.chosen": {
      "exports": null,
      "depends": [
        "jquery:jQuery"
      ]
    },
    "jquery.throttle-debounce": {
      "exports": null,
      "depends": [
        "jquery:$"
      ]
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "dependencies": {
    "async": "^2.0.0-rc.2",
    "aws-sdk": "^2.3.13",
    "body-parser": "^1.15.0",
    "bower": "^1.7.9",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "cloudinary": "^1.3.1",
    "compression": "^1.6.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.1",
    "crypto": "0.0.3",
    "dbox": "^0.6.4",
    "elasticsearch": ">=1.1.0",
    "express": "4.13.3",
    "express-handlebars": "^3.0.0",
    "express-react-views": "^0.10.2",
    "express-session": "^1.13.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-declare": "^0.3.0",
    "gulp-handlebars": "^4.0.0",
    "gulp-inject-string": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-uglify": "^1.5.3",
    "gulp-watch": "^4.3.5",
    "gulp-wrap": "^0.11.0",
    "gulp.spritesmith": "^6.2.1",
    "handlebars": "^4.0.0",
    "lodash": "^4.11.1",
    "merge-stream": "^1.0.0",
    "mongoose": "^4.4.8",
    "mongoose-auto-increment": "^5.0.1",
    "multer": "^1.1.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-remember-me": "0.0.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "rollbar": "^0.6.0",
    "run-sequence": "^1.2.1",
    "s3fs": "^2.4.7",
    "serve-favicon": "^2.3.0",
    "sparkpost": "^1.3.3",
    "vinyl-source-stream": "^1.1.0"
  },
  "engines": {
    "node": "7.0.0",
    "npm": "3.10.x"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
